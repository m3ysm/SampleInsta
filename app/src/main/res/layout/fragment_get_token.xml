<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context=".ui.gettoken.GetTokenFragment">

    <WebView
        android:id="@+id/webView_getTokenFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/margin_all_20dp" />

    <com.example.sample.widget.ProgressLayout
        android:id="@+id/progressLayout_getTokenFragment"
        android:layout_width="@dimen/width_all_0dp"
        android:layout_height="@dimen/dimension_all_0dp"
        android:elevation="@dimen/elevation_all_4dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>